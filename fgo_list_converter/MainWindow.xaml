<Window x:Class="fgo_list_converter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fgo_list_converter"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel LastChildFill="False">
        <DockPanel DockPanel.Dock="Top" Margin="5">
            <TextBlock DockPanel.Dock="Top" Text="https://www9.atwiki.jp/f_go/pages/160.html の表を再利用可能な形にパースする手抜きツール"/>
            <TextBlock DockPanel.Dock="Top" Text="入力はカレントフォルダのInput.txt。出力はカレントフォルダのOutput.csv。"/>
            <Button DockPanel.Dock="Top" Content="コンバートCSV" Click="StartConvert"/>
        </DockPanel>
    </DockPanel>
</Window>
